{
  "name": "tree-sitter-javadoc",
  "version": "0.2.3",
  "description": "Parser for Javadoc documentation comments",
  "repository": "https://github.com/rmuir/tree-sitter-javadoc",
  "license": "MIT",
  "author": {
    "name": "Robert Muir",
    "email": "rmuir@apache.org",
    "url": "https://github.com/rmuir"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "javadoc"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.2"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.25.8",
    "tree-sitter": "^0.25.0",
    "eslint": "^9.33.0",
    "eslint-config-treesitter": "^1.0.2",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "tree-sitter": "^0.25.0"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "lint": "eslint --max-warnings 0 grammar.js",
    "typecheck": "tsc --noEmit",
    "test": "node --test bindings/node/*_test.js"
  }
}
